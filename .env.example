# Environment configuration - Copy this file to .env and update with your values
# This file is used by the Python scripts to connect to the database

# Environment selection (dev, preprod, or prod)
ENV=dev

# Database connection parameters
DB_USER=myuser
DB_PASSWORD=your-secure-password

# Google Cloud configuration
GCP_PROJECT_ID=sandbox-pgvector-project1
GCP_LOCATION=us-central1

# Embedding model
EMBEDDING_MODEL=text-embedding-004
